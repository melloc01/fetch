{
    "name": "rcbytes/fetch",
    "description": "fetch your eloquent models",
    "keywords": [
        "laravel",
        "eloquent",
        "fetch"
    ],
    "homepage": "https://github.com/rcbytes/fetch",
    "license": "MIT",
    "authors": [
        {
            "name": "Rafael Mello Campanari",
            "email": "lab.rafamello@gmail.com",
            "homepage": "http://rafamello.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "rcbytes\\fetch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rcbytes\\fetch\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
